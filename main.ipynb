    "from sage.all import *\n",
    "\n",
    "## y^2 = x^3 + 497x + 1768 (mod 9739)\n",
    "\n",
    "from Crypto.Util.number import *\n",
    "\n",
    "p = 9739\n",
    "assert isPrime(p)\n",
    "\n",
    "a = 497\n",
    "b = 1768\n",
    "\n",
    "E = EllipticCurve(GF(p), [a, b])\n",
    "\n",
    "G = E.gens()\n",
    "\n",
    "print(G)"
   ]
  }
 ],
